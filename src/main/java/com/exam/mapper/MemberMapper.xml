<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.exam.mapper.MemberMapper">


 
 <resultMap id="MemberResultMap" type="com.exam.entity.Member">
        <id property="userid" column="userid" />
        <result property="password" column="password" />
        <result property="email" column="email" />
        <result property="address" column="address" />
    </resultMap>

    <select id="findByUsername" parameterType="String" resultMap="MemberResultMap">
        SELECT * FROM Member WHERE userid = #{userid}
    </select>

    <insert id="insertMember" parameterType="com.exam.entity.Member">
        INSERT INTO Member (userid, password, email, address)
        VALUES (#{userid}, #{password}, #{email}, #{address})
    </insert>
</mapper>


